/**app.wxss**/
@import "/lib/style/iconfont.wxss";
.container {
  box-sizing: border-box;
  background-color: #f4f4f4;
  font-family: PingFangSC-Light, helvetica, 'Heiti SC';
}

view, image, text, navigator {
  box-sizing: border-box;
  padding: 0;
  margin: 0;
}

view, text {
  font-family: PingFangSC-Light, helvetica, 'Heiti SC';
  font-size: 29rpx;
  color: #333;
}

.slant {
  margin:0 8rpx;
}

.commission {
  font-size: 28rpx;
  color: #FF7128;
}
